         <rnews:description xml:lang="en-GB">A former North Vietnamese soldier is reunited with his arm after more than 40 years, when the US war doctor who amputated it returned the bones in person.</rnews:description>
         <rnews:datePublished rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2013-07-01T11:57:13</rnews:datePublished>
         <rnews:thumbnailUrl rdf:resource="http://news.bbcimg.co.uk/media/images/68480000/jpg/_68480012_68479502.jpg"/>
         <rnews:headline xml:lang="en-GB">Vietnam man regains long-lost arm</rnews:headline>
         <rnews:articleSection rdf:datatype="http://www.w3.org/2001/XMLSchema#string">World</rnews:articleSection>
         <rnews:creator rdf:resource="http://www.bbc.co.uk#org"/>
      </rdf:Description>
